<Window x:Class="metronome.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:metronome"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="225">
    <Grid>
        <RadioButton x:Name="radioButton" Content="" HorizontalAlignment="Left" Margin="103,46,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="23" Margin="83,115,0,0" TextWrapping="Wrap" Text="20" VerticalAlignment="Top" Width="54"/>
        <Button x:Name="button" Content="+" HorizontalAlignment="Left" Margin="152,115,0,0" VerticalAlignment="Top" Width="25" Height="23" Click="button_Click"/>
        <Button x:Name="button_Copy" Content="-" HorizontalAlignment="Left" Margin="42,115,0,0" VerticalAlignment="Top" Width="25" Height="23" RenderTransformOrigin="-2.936,0.722" Click="button_Copy_Click"/>
        <Button x:Name="Start" Content="Button" HorizontalAlignment="Left" Margin="42,204,0,0" VerticalAlignment="Top" Width="135" Click="Start_Click"/>

    </Grid>
</Window>
